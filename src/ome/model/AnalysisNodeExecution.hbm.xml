<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.AnalysisNodeExecution" 
    table="analysis_node_executions"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="analysisNodeExecutionId"
        type="java.lang.Integer"
        column="analysis_node_execution_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>


    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AnalysisChainExecution -->
    <many-to-one
        name="analysisChainExecution"
        class="ome.model.AnalysisChainExecution"
        not-null="true"
    >
        <column name="analysis_chain_execution_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to AnalysisChainNode -->
    <many-to-one
        name="analysisChainNode"
        class="ome.model.AnalysisChainNode"
        not-null="true"
    >
        <column name="analysis_chain_node_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecution"
        class="ome.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>

</class>
</hibernate-mapping>
