<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.SemanticElement" 
    table="semantic_elements"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="semanticElementId"
        type="java.lang.Integer"
        column="semantic_element_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="64"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="-1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to SemanticType -->
    <many-to-one
        name="semanticType"
        class="ome.model.SemanticType"
        not-null="true"
    >
        <column name="semantic_type_id" />
    </many-to-one>

</class>
</hibernate-mapping>
