<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.FormalInput" 
    table="formal_inputs"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="formalInputId"
        type="java.lang.Integer"
        column="formal_input_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="userDefined"
        type="java.lang.Boolean"
        column="user_defined"
        length="1"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="64"
    />
    <property
        name="optional"
        type="java.lang.Boolean"
        column="optional"
        length="1"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="-1"
    />
    <property
        name="list"
        type="java.lang.Boolean"
        column="list"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Module -->
    <many-to-one
        name="module"
        class="ome.model.Module"
        not-null="true"
    >
        <column name="module_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to SemanticType -->
    <many-to-one
        name="semanticType"
        class="ome.model.SemanticType"
        not-null="true"
    >
        <column name="semantic_type_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to AnalysisChainLink -->
    <set
        name="analysisChainLinks"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="to_input" />
        </key>
        <one-to-many 
            class="ome.model.AnalysisChainLink"
        />
    </set>

</class>
</hibernate-mapping>
