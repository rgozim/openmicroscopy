<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.Group" 
    table="groups"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="sequence">
            <param name="sequence">attribute_seq</param>
        </generator>
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="30"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Dataset -->
    <set
        name="datasets"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.Dataset"
        />
    </set>
    <!-- bi-directional one-to-many association to Image -->
    <set
        name="images"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.Image"
        />
    </set>
    <!-- bi-directional one-to-many association to Project -->
    <set
        name="projects"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.Project"
        />
    </set>
    <!-- bi-directional one-to-many association to Experimenter -->
    <set
        name="experimenters"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.Experimenter"
        />
    </set>
    <!-- bi-directional one-to-many association to ModuleExecution -->
    <set
        name="moduleExecutions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.ModuleExecution"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageInfo -->
    <set
        name="imageInfos"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="ome.model.ImageInfo"
        />
    </set>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="leader"
        class="ome.model.Experimenter"
        not-null="true"
    >
        <column name="leader" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="contact"
        class="ome.model.Experimenter"
        not-null="true"
    >
        <column name="contact" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecution"
        class="ome.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>


</class>
</hibernate-mapping>
