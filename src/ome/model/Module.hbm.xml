<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.Module" 
    table="modules"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="moduleId"
        type="java.lang.Integer"
        column="module_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="location"
        type="java.lang.String"
        column="location"
        not-null="true"
        length="128"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        length="64"
    />
    <property
        name="executionInstructions"
        type="java.lang.String"
        column="execution_instructions"
        length="-1"
    />
    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="-1"
    />
    <property
        name="moduleType"
        type="java.lang.String"
        column="module_type"
        not-null="true"
        length="128"
    />
    <property
        name="defaultIterator"
        type="java.lang.String"
        column="default_iterator"
        length="128"
    />
    <property
        name="newFeatureTag"
        type="java.lang.String"
        column="new_feature_tag"
        length="128"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to FormalOutput -->
    <set
        name="formalOutputs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_id" />
        </key>
        <one-to-many 
            class="ome.model.FormalOutput"
        />
    </set>
    <!-- bi-directional one-to-many association to FormalInput -->
    <set
        name="formalInputs"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_id" />
        </key>
        <one-to-many 
            class="ome.model.FormalInput"
        />
    </set>
    <!-- bi-directional one-to-many association to ModuleExecution -->
    <set
        name="moduleExecutions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_id" />
        </key>
        <one-to-many 
            class="ome.model.ModuleExecution"
        />
    </set>
    <!-- bi-directional one-to-many association to AnalysisChainNode -->
    <set
        name="analysisChainNodes"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_id" />
        </key>
        <one-to-many 
            class="ome.model.AnalysisChainNode"
        />
    </set>

</class>
</hibernate-mapping>
