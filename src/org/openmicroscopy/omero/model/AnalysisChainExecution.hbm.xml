<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.AnalysisChainExecution" 
    table="analysis_chain_executions"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="analysisChainExecutionId"
        type="java.lang.Integer"
        column="analysis_chain_execution_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="timestamp"
        type="java.sql.Timestamp"
        column="timestamp"
        length="8"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to AnalysisChain -->
    <many-to-one
        name="analysisChain"
        class="org.openmicroscopy.omero.model.AnalysisChain"
        not-null="true"
    >
        <column name="analysis_chain_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to AnalysisNodeExecution -->
    <set
        name="analysisNodeExecutions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="analysis_chain_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.AnalysisNodeExecution"
        />
    </set>
    <!-- bi-directional many-to-one association to Dataset -->
    <many-to-one
        name="dataset"
        class="org.openmicroscopy.omero.model.Dataset"
        not-null="true"
    >
        <column name="dataset_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenter"
        class="org.openmicroscopy.omero.model.Experimenter"
        not-null="true"
    >
        <column name="experimenter_id" />
    </many-to-one>

</class>
</hibernate-mapping>
