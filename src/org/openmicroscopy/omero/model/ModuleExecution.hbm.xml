<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.ModuleExecution" 
    table="module_executions"
>

    <id
        name="moduleExecutionId"
        type="java.lang.Integer"
        column="module_execution_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="status"
        type="java.lang.String"
        column="status"
        length="16"
    />
    <property
        name="attributeDbTime"
        type="java.lang.Double"
        column="attribute_db_time"
        length="8"
    />
    <property
        name="virtualMex"
        type="boolean"
        column="virtual_mex"
        not-null="true"
        length="1"
    />
    <property
        name="inputTag"
        type="java.lang.String"
        column="input_tag"
        length="-1"
    />
    <property
        name="iteratorTag"
        type="java.lang.String"
        column="iterator_tag"
        length="128"
    />
    <property
        name="dependence"
        type="java.lang.String"
        column="dependence"
        not-null="true"
        length="1"
    />
    <property
        name="attributeSortTime"
        type="java.lang.Double"
        column="attribute_sort_time"
        length="8"
    />
    <property
        name="errorMessage"
        type="java.lang.String"
        column="error_message"
        length="-1"
    />
    <property
        name="timestamp"
        type="java.sql.Timestamp"
        column="timestamp"
        length="8"
    />
    <property
        name="attributeCreateTime"
        type="java.lang.Double"
        column="attribute_create_time"
        length="8"
    />
    <property
        name="newFeatureTag"
        type="java.lang.String"
        column="new_feature_tag"
        length="128"
    />
    <property
        name="totalTime"
        type="java.lang.Double"
        column="total_time"
        length="8"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.openmicroscopy.omero.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Dataset -->
    <many-to-one
        name="dataset"
        class="org.openmicroscopy.omero.model.Dataset"
        not-null="true"
    >
        <column name="dataset_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenter"
        class="org.openmicroscopy.omero.model.Experimenter"
        not-null="true"
    >
        <column name="experimenter_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to Thumbnail -->
    <set
        name="thumbnails"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Thumbnail"
        />
    </set>
    <!-- bi-directional one-to-many association to Classification -->
    <set
        name="classifications"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Classification"
        />
    </set>
    <!-- bi-directional one-to-many association to Category -->
    <set
        name="categories"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.Category"
        />
    </set>
    <!-- bi-directional one-to-many association to DisplayRoi -->
    <set
        name="displayRois"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DisplayRoi"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageInfo -->
    <set
        name="imageInfos"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageInfo"
        />
    </set>
    <!-- bi-directional one-to-many association to ImagePixel -->
    <set
        name="imagePixels"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImagePixel"
        />
    </set>
    <!-- bi-directional one-to-many association to ImagePlate -->
    <set
        name="imagePlates"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImagePlate"
        />
    </set>
    <!-- bi-directional one-to-many association to CategoryGroup -->
    <set
        name="categoryGroups"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.CategoryGroup"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageAnnotation -->
    <set
        name="imageAnnotations"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageAnnotation"
        />
    </set>
    <!-- bi-directional one-to-many association to DatasetAnnotation -->
    <set
        name="datasetAnnotations"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DatasetAnnotation"
        />
    </set>
    <!-- bi-directional one-to-many association to ImageDimension -->
    <set
        name="imageDimensions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ImageDimension"
        />
    </set>
    <!-- bi-directional one-to-many association to ChannelComponent -->
    <set
        name="channelComponents"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ChannelComponent"
        />
    </set>
    <!-- bi-directional one-to-many association to DisplayOption -->
    <set
        name="displayOptions"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="module_execution_id" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DisplayOption"
        />
    </set>

</class>
</hibernate-mapping>
