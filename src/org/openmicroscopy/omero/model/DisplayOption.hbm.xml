<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.DisplayOption" 
    table="display_options"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="redOn"
        type="java.lang.Boolean"
        column="red_on"
        length="1"
    />
    <property
        name="ZStart"
        type="java.lang.Integer"
        column="z_start"
        length="4"
    />
    <property
        name="blueOn"
        type="java.lang.Boolean"
        column="blue_on"
        length="1"
    />
    <property
        name="colorMap"
        type="java.lang.String"
        column="color_map"
        length="-1"
    />
    <property
        name="ZStop"
        type="java.lang.Integer"
        column="z_stop"
        length="4"
    />
    <property
        name="zoom"
        type="java.lang.Float"
        column="zoom"
        length="4"
    />
    <property
        name="TStop"
        type="java.lang.Integer"
        column="t_stop"
        length="4"
    />
    <property
        name="TStart"
        type="java.lang.Integer"
        column="t_start"
        length="4"
    />
    <property
        name="greenOn"
        type="java.lang.Boolean"
        column="green_on"
        length="1"
    />
    <property
        name="displayRgb"
        type="java.lang.Boolean"
        column="display_rgb"
        length="1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.openmicroscopy.omero.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ImagePixel -->
    <many-to-one
        name="imagePixel"
        class="org.openmicroscopy.omero.model.ImagePixel"
        not-null="true"
    >
        <column name="pixels" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecution"
        class="org.openmicroscopy.omero.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to DisplayRoi -->
    <set
        name="displayRois"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="display_options" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.DisplayRoi"
        />
    </set>

</class>
</hibernate-mapping>
