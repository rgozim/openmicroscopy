<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.OmeSession" 
    table="ome_sessions"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="sessionId"
        type="java.lang.Integer"
        column="session_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="sessionKey"
        type="java.lang.String"
        column="session_key"
        length="40"
    />
    <property
        name="moduleExecutionId"
        type="java.lang.Integer"
        column="module_execution_id"
        length="4"
    />
    <property
        name="featureView"
        type="java.lang.String"
        column="feature_view"
        length="-1"
    />
    <property
        name="host"
        type="java.lang.String"
        column="host"
        length="256"
    />
    <property
        name="lastAccess"
        type="java.sql.Timestamp"
        column="last_access"
        length="8"
    />
    <property
        name="started"
        type="java.sql.Timestamp"
        column="started"
        length="8"
    />
    <property
        name="imageView"
        type="java.lang.String"
        column="image_view"
        length="-1"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Project -->
    <many-to-one
        name="project"
        class="org.openmicroscopy.omero.model.Project"
        not-null="true"
    >
        <column name="project_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Dataset -->
    <many-to-one
        name="dataset"
        class="org.openmicroscopy.omero.model.Dataset"
        not-null="true"
    >
        <column name="dataset_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenter"
        class="org.openmicroscopy.omero.model.Experimenter"
        not-null="true"
    >
        <column name="experimenter_id" />
    </many-to-one>

</class>
</hibernate-mapping>
