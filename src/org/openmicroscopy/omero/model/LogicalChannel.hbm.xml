<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.openmicroscopy.omero.model.LogicalChannel" 
    table="logical_channels"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>

    <property
        name="photometricInterpretation"
        type="java.lang.String"
        column="photometric_interpretation"
        length="-1"
    />
    <property
        name="mode"
        type="java.lang.String"
        column="mode"
        length="-1"
    />
    <property
        name="auxLightAttenuation"
        type="java.lang.Float"
        column="aux_light_attenuation"
        length="4"
    />
    <property
        name="exWave"
        type="java.lang.Integer"
        column="ex_wave"
        length="4"
    />
    <property
        name="detectorOffset"
        type="java.lang.Float"
        column="detector_offset"
        length="4"
    />
    <property
        name="auxTechnique"
        type="java.lang.String"
        column="aux_technique"
        length="-1"
    />
    <property
        name="fluor"
        type="java.lang.String"
        column="fluor"
        length="-1"
    />
    <property
        name="contrastMethod"
        type="java.lang.String"
        column="contrast_method"
        length="-1"
    />
    <property
        name="detectorGain"
        type="java.lang.Float"
        column="detector_gain"
        length="4"
    />
    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="-1"
    />
    <property
        name="samplesPerPixel"
        type="java.lang.Integer"
        column="samples_per_pixel"
        length="4"
    />
    <property
        name="lightAttenuation"
        type="java.lang.Float"
        column="light_attenuation"
        length="4"
    />
    <property
        name="emWave"
        type="java.lang.Integer"
        column="em_wave"
        length="4"
    />
    <property
        name="auxLightWavelength"
        type="java.lang.Integer"
        column="aux_light_wavelength"
        length="4"
    />
    <property
        name="illuminationType"
        type="java.lang.String"
        column="illumination_type"
        length="-1"
    />
    <property
        name="ndFilter"
        type="java.lang.Float"
        column="nd_filter"
        length="4"
    />
    <property
        name="pinholeSize"
        type="java.lang.Integer"
        column="pinhole_size"
        length="4"
    />
    <property
        name="lightWavelength"
        type="java.lang.Integer"
        column="light_wavelength"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to ChannelComponent -->
    <set
        name="channelComponents"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="logical_channel" />
        </key>
        <one-to-many 
            class="org.openmicroscopy.omero.model.ChannelComponent"
        />
    </set>
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.openmicroscopy.omero.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecution"
        class="org.openmicroscopy.omero.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>

</class>
</hibernate-mapping>
