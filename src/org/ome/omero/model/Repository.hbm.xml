<class 
    name="org.ome.omero.model.Repository" 
    table="repositories"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="moduleExecution"
        type="org.ome.omero.model.ModuleExecution"
        column="module_execution_id"
        length="4"
    />
    <property
        name="imageServerUrl"
        type="java.lang.String"
        column="image_server_url"
        not-null="true"
        unique="true"
        length="-1"
    />
    <property
        name="path"
        type="java.lang.String"
        column="path"
        unique="true"
        length="256"
    />
    <property
        name="isLocal"
        type="java.lang.Boolean"
        column="is_local"
        length="1"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
