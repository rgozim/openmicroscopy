<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

  <filter-def name="ownerFilter">
    <filter-param type="Long" name="ownerId"/>
  </filter-def>
  
  <filter-def name="groupFilter">
    <filter-param type="Long" name="groupId"/>
  </filter-def>
  
  <filter-def name="eventFilter">
  	<filter-param type="Long" name="eventId"/>  
  </filter-def>
  
  <filter-def name="permsFilter">
    <filter-param type="String" name="permsStr"/>
  </filter-def>
  
</hibernate-mapping>
