<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.core.Roi2D" 
    table="roi2d"
>
	<meta attribute="scope-class" inherit="false">public abstract</meta>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="roi2d"
    </meta>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="sequence"
            type="java.lang.Integer"
            column="id"

           @hibernate.generator-param
	        name="sequence"
	        value="roi2d_seq"
        </meta>
        <generator class="sequence">
            <param name="sequence">roi2d_seq</param>
        </generator>
    </id>
	
    <discriminator>
      <column name="type" sql-type="char(5)"/>
    </discriminator>
	
    <version
        name="version"
        type="java.lang.Integer"
        column="version"
        unsaved-value="negative"
    >
        <meta attribute="field-description">
           @hibernate.version
            column="version"
            unsaved-value="negative"
        </meta>
    </version>
	
    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Pixel -->
    <many-to-one
        name="roi3d"
        class="ome.model.core.Roi3D"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="roi3d"         
        </meta>
        <column name="roi3d" />
    </many-to-one>

	<many-to-one
        name="shape"
        class="ome.model.core.Shape"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="shape"         
        </meta>
        <column name="shape" />
    </many-to-one>
	
    <subclass name="ome.model.core.Roi2DZRange" discriminator-value="range">
      <property name="minZ" column="min_z" type="java.lang.Integer" />
      <property name="maxZ" column="max_z" type="java.lang.Integer" />
     </subclass>

    <subclass name="ome.model.core.Roi2DZIndex" discriminator-value="index">
	  <property name="indexZ" column="index_z" type="java.lang.Integer"/>
    </subclass>
	
</class>
</hibernate-mapping>
