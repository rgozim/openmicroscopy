<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.core.Eg" 
    table="eg"
>
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="sequence">
            <param name="sequence">eq_seq</param>
        </generator>
    </id>

	
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 COMMON
     ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <version
        name="version"
        type="java.lang.Integer"
        column="version"
        unsaved-value="negative"
    >
        <meta attribute="field-description">
           @hibernate.version
            column="version"
            unsaved-value="negative"
        </meta>
    </version>

	<property name="permissions" type="ome.hibernate.PermissionsType"/>
    <many-to-one name="owner" class="ome.model.meta.Experimenter" not-null="true" cascade="save-update"/>
    <many-to-one name="creationEvent" class="ome.model.meta.Event" not-null="true" cascade="save-update"/>		
    <many-to-one name="updateEvent" class="ome.model.meta.Event" not-null="false"/>	
	
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 SPECIFIC
     ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	
	
    <property name="list" type="ome.hibernate.ArrayStringType"/>
	
</class>
</hibernate-mapping>
