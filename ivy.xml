<ivy-module version="1.0">
  <info
      organisation="omero"
      module="common"
      status="integration"/>
  <configurations>
    <conf name="core"/>
    <conf name="build" extends="core"/>
    <conf name="test"  extends="core"/>
  </configurations>
  <publications>
    <artifact name="common" type="jar" conf="core" />
  </publications>
  <dependencies>
    <!-- Internal -->
    <dependency name="model" rev="${omero.version}" changing="true" />
    <!-- Public -->
    <dependency org="jamon" name="jamon" rev="${versions.jamon}" conf="test->default"/> 
    <dependency org="net/sf/ehcache" name="ehcache" rev="${versions.ehcache}" conf="build->default"/>
  </dependencies>
</ivy-module>
