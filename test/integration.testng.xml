<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
  
<suite name="server.integration" >
  
  <test name="server.integration.active">
    <groups>
      <run>
        <exclude name="broken"/>
        <exclude name="ignore"/>
        <exclude name="client"/>
        <exclude name="blitz"/>
        <exclude name="manual"/>
      </run>        
    </groups>
    <packages>
      <package name="ome.io.*"/>
      <package name="ome.server.*"/>
      <package name="ome.services.*"/>
      <package name="omeis.*"/>
    </packages>
  </test>

</suite>
